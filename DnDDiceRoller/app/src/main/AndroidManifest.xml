<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dnddiceroller">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.DnDDiceRoller">
        <activity
            android:name="com.example.dnddiceroller.Calculator"
            android:parentActivityName="com.example.dnddiceroller.MainActivity" />
        <activity
            android:name="com.example.dnddiceroller.Result"
            android:parentActivityName="com.example.dnddiceroller.MainActivity" />
        <activity android:name="com.example.dnddiceroller.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>